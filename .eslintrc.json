{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        //"max-len": ["error", { "code": 150 }],
        "max-len": "off",
        "no-console": "off",
        "global-require":"off",
        "no-use-before-define": "off"
    }
}
